	name = Francisco Carpio
	email = carpiofj@gmail.com
[credential]
	helper = store
[core]
    pager = delta

[interactive]
    diffFilter = delta --color-only

[delta]
    navigate = true    # use n and N to move between diff sections
    light = false      
	side-by-side = true
	line-numbers = true

[merge]
    conflictstyle = diff3

[diff]
    colorMoved = default


; [url "git@github.com:"]
	; insteadOf = https://github.com/
; [filter "lfs"]
	; process = git-lfs filter-process
	; required = true
	; clean = git-lfs clean -- %f
	; smudge = git-lfs smudge -- %f
